<h1>Register Book</h1>
<p>Find me in app/views/books/new.html.erb</p>

<%= form_for(@book) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :author %>
  <%= f.text_field :author %>

  <%= f.label :isbn %>
  <%= f.text_field :isbn %>

  <%= f.label :quantity %>
  <%= f.text_field :quantity %>

  <%= f.submit "Register book" %>
<% end %>
